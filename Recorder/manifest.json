{
	"name": "Recorder",
	"version": "1.0",
	"manifest_version": 2,
	"description": "",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "./icons/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"loadTime.js",
				"explore.js",
				"utils.js"
			]
		}
	],
	"permissions": [
		"activeTab",
		"contextMenus",
		"storage"
	]
}
